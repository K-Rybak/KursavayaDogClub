@model IEnumerable<KursavayaDogClub.Models.BREED>

@{
    ViewBag.Title = "Index";
}

<h2>Список пород</h2>

<ul class="pager">
    <li class="previous">@Html.ActionLink("Добавить запись", "Create")</li>
</ul>
<table class="table">
    <tr>
        <th>
            @Html.Label("Порода")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BREED_NAME)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id = item.BREED_ID }) |
            @Html.ActionLink("Подробнее", "Details", new { id = item.BREED_ID }) |
            @Html.ActionLink("Удалить", "Delete", new { id = item.BREED_ID })
        </td>
    </tr>
}

</table>
